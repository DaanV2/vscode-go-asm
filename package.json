{
  "description": "An extension that shows you assembly code of you go code",
  "displayName": "Go ASM Preview",
  "license": "BSD-3-Clause",
  "name": "daanv2-go-asm",
  "icon": "resources/icon.png",
  "pricing": "Free",
  "publisher": "DaanV2",
  "version": "0.1.1",
  "author": {
    "name": "DaanV2"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DaanV2/vscode-go-asm"
  },
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "workspace"
  ],
  "activationEvents": [
    "onLanguage:go",
    "onLanguage:golang",
    "onCommand:daanv2-go-asm.show-assembly"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "daanv2-go-asm.show-assembly",
        "title": "Show Go Assembly",
        "enablement": "editorLangId == 'go'"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "daanv2-go-asm.show-assembly",
          "group": "Go",
          "when": "editorLangId == 'go'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "echo no test yet"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "esbuild": "^0.25.8",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  }
}
